class Solution {
// Check if strings are same length, if they arent then return false
// then put strings into char arrays, sort the arrays
// return true or false if the two arrays are equal or not

    public boolean isAnagram(String s, String t) {

        if(s.length() != t.length()) {
            return false;
        }
        char [] sortedS = s.toCharArray();
        char [] sortedT = t.toCharArray();

        Arrays.sort(sortedS);
        Arrays.sort(sortedT);

 return Arrays.equals(sortedS, sortedT);
    } 
}
